              <h2>Product List</h2>


              <form action="/admin/products" method="GET">
                <select name="category">
                  <option value="">All Categories</option>
                  <% for(let i=0; i<categories.length; i++) { %>
                    <option value="<%= categories[i]._id %>"><%= categories[i].name %></option>
                  <% } %>
                </select>
                <button type="submit">Filter</button>
              </form>
            <br>
            <br>

              <table id="dataTable">
                <thead>
                  <tr>
                    <th>Image</th>
                    <th>Product Name</th>
                    <th>Price</th>
                    <th>Quantity</th>
                    <th>Category</th>
                    <th></th>
                    <th>Offer</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  <% if(Products.length > 0) { %>
                    <% for(let i=0; i<Products.length; i++) { %>
                      <tr>
                        <td>
                          <img src="/productImage/<%= Products[i].Image[0] %>" width="50px" height="35px" alt="">
                        </td>
                        <td><%= Products[i].name %></td>
                        <td>₹<%= Products[i].price %></td>
                        <td><%= Products[i].quantity %></td>
                        <td><%= Products[i].category.name %></td>
                        <td>
                          <button class="btn-1">
                            <a href="/admin/editproduct?productId=<%= Products[i]._id %>">Edit</a>
                          </button>
                          <button class="btn-2">
                            <a href="/admin/deleteproduct?id=<%= Products[i]._id %>">Delete</a>
                          </button>
                        </td>
                        <td>
                          <% if(Products[i].offer[0]) { %>
                            <button class="btn-2" style="color: #fff;" onclick="removeoffer('<%= Products[i].offer[0]._id %>','<%=Products[i]._id %>')">
                              <a>Remove</a>
                            </button>
                          <% } else { %>
                            <button type="button" style="color: #fff;background-color: #2f971a;" class="btn-1" onclick="modalshow('<%= Products[i]._id %>')">
                              Select
                            </button>
                          <% } %>
                        </td>
                        <td>
                          <% if(Products[i].is_Listed == false) { %>
                            <form action="/admin/Listproduct" method="post">
                              <input type="hidden" name="id" value="<%= Products[i]._id %>">
                              <button type="submit" class="btn-3">
                                <i>List</i>
                              </button>
                            </form>
                          <% } else { %>
                            <form action="/admin/Unlistproduct" method="post">
                              <input type="hidden" name="id" value="<%= Products[i]._id %>">
                              <button type="submit" class="btn-4">
                                <i>Unlist</i>
                              </button>
                            </form>
                          <% } %>
                        </td>
                      </tr>
                    <% } %>
                  <% } else { %>
                    <tr>
                      <td colspan="8">No products found</td>
                    </tr>
                  <% } %>
                </tbody>
              </table>
           <br>
           <br>
          
          
          











              <div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                <div class="modal-dialog">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h1 class="modal-title fs-5" id="staticBackdropLabel">SELECT OFFER</h1>
                      <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                   
                    <div class="modal-body">
                      <% if(offers.length > 0) { %>
                        <% offers.forEach((offer)=>{ %>
                      <div class="card" style="width: 18rem;">
                        <div class="card-body">
                          <h5 class="card-title"><%=offer.name %></h5>
                          <p class="card-text" style="color: #fff;"><%=offer.discount%></p>
                          <p class="card-text" style="color: #fff;" ><%=offer.startingdate.toLocaleDateString('en-US', {
                            weekday: 'short',
                            month: 'short',
                            day: '2-digit',
                            year: 'numeric'
                          }).toLowerCase() %></p>
                          <p class="card-text" style="color: #fff;" ><%=offer.endingdate.toLocaleDateString('en-US', {
                            weekday: 'short',
                            month: 'short',
                            day: '2-digit',
                            year: 'numeric'
                          }).toLowerCase() %></p>

                          <a type="button" class="btn btn-primary apply-offer-btn " data-offerid="<%=offer._id%>">apply offer</a>
                        </div>
                      </div>
                  <% }) %>
                <% }else{ %>
                <h5 style="color: #fff;">No Offer Found</h5>
                <%}%>
                    </div>
                    <div class="modal-footer">
                      <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                      
                    </div>
                  </div>
