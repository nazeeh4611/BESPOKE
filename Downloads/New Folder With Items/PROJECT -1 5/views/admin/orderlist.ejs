                      <th>PaymentMethod</th>
                      <th class="action-column">Items</th>
                      <th>Order details</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% if(Orders.length> 0){ %>
                      <% for(let i=0; i < Orders.length; i++){ %>
                        <tr>
                          <td>
                            <%= i + 1 %>
                          </td>
                          <td>
                            <%= Orders[i]._id %>
                          </td>
                          <td>
                            <%= Orders[i].subtotal  %>
                          </td>
                          <td>
                            <%= Orders[i].paymentMethod %>
                          </td>
                      
                      <td> <%= Orders[i].product.length %></td>
                       
                        <td>
                            <a href="/admin/orders?id=<%= Orders[i]._id %>"><button style="color: #ffffff;background-color: rgb(0, 201, 27);">view details</button></a>
                          </td>
                      
                        </tr>
                        <% } %>
                          <% } else { %>
                            <tr>
                              <td colspan="5">Orders Not Found</td>
                            </tr>
                            <% } %>
                  </tbody>
                </table>
