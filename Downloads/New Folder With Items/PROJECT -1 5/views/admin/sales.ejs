  /* Table styles */
  table {
    width: 100%;
    border-collapse: collapse;
  }

  th,
  td {
    padding: 8px;
    border: 1px solid #505050;
    text-align: left;
  }

  th {
    background-color: #000000;
    color: rgb(157, 157, 157);
  }

  /* Button styles */
  .btn-1,
  .btn-2,
  .btn-3,
  .btn-4 {
    padding: 2px 8px;
    margin: 2px;
    cursor: pointer;
    border: none;
    border-radius: 4px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
  }

  .btn-1 a,
  .btn-2 a {
    color: #fff;
    text-decoration: none;
  }

  .btn-1 {
    background-color: #2f971a;
    /* Blue for Edit button */
  }

  .btn-2 {
    background-color: #dc3545;
    /* Red for Delete button */
  }

  .btn-3 {
    background-color: #28a745;
    /* Green for List button */
  }

  .btn-4 {
    background-color: #dc3545;
    /* Red for Unlist button */
  }

  .btn-7 {
    padding: 3px 6px;
    margin-right: 20px;
    /* Add margin to create space between the button and the heading */
    background-color: #28a745;
    /* Green background color */
    color: white;
    /* White text color */
    border: none;
    border-radius: 4px;
    cursor: pointer;
    transition: background-color 0.3s ease;
  }

  .btn-7:hover {
    background-color: #076e1f;
    /* Darker green on hover */
  }


  h2 {
    display: inline-block;
    /* Keep the heading and button on the same line */
