            <h2>SALES REPORT</h2>
            <div class="row">
           
           
              <br>
              <form method="post">
                <div class="startDate">
                    Starting Date: &nbsp; <input type="date" name="fromdate" id="dat" style="background-color: #fff; border: none ; color: #000000; padding-top: 3px; border-color: white">                
                    &nbsp;  Ending Date:   <input type="date" name="todate" id="date" style="background-color: #fff; border: none ; color: #000000; padding-top: 3px; border-color: white">
                </div>
                <button type="submit">filter</button>
            </form>
              <table>
                <thead>
                  <tr>
                    <th>OrderID</th>
                    <th>Product-Details</th>
                    <th>Date</th>
                    <th>Payment-Method</th>
                    <th>Status</th>
                    <th>Total Amount</th>
                    <th>Customer Name </th>
                  </tr>
                </thead>
           <% if(orderData ) { %>
           <% orderData.forEach((order)=> {
            order.product.forEach((item) => {
               
             %>
                      <tr>
                        <td>
                         <%= order._id %>
                        </td>
                        <td>
                      
                       <%=item.name %>
                        </td>
                        <td>
                      <%= order.Date.toLocaleDateString('en-US', {
                        weekday: 'short',
                        month: 'short',
                        day: '2-digit',
                        year: 'numeric'
                      }).toLowerCase() %>
                        </td>
                        <td>
                            <%= order.paymentMethod %>
                        </td>
                        <td>
                            <%= order.status %>
                        </td>
                        <td>
                          ₹<%= order.subtotal %>
                        </td>
                        <td>
                            <%= order.user.name %>
                        </td>
                     
                      </tr>
                      <%}) %>
                      <% }) %>
                      <% }else{ %>
                          <tr>
                            <td colspan="5">Offer is not found</td>
                          </tr>
                          <% } %>


                </tbody>
              </table>
      
      
