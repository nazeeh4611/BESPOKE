<script>
function modal(productId,status,orderId) {
  console.log("hiii",productId,status,orderId)
 
    // Assuming you're using jQuery for AJAX requests
    $.ajax({
        url: '/getReason', // Route to fetch reason based on product ID
        type: 'GET',
        data: { productId: productId,
               orderId:orderId },
        success: function(data) {
            // Populate modal body with the fetched reason
            $('.modal-body').html(data.reason);
            console.log("kiii",data.reason)
            // Display the modal
            $('#staticB').modal('show');
          returned(status,orderId,productId)

        },
        error: function(err) {
            console.error('Error fetching reason:', err);
        }
    });
}

 


</script>
<script>
  function shipmentUpdate(status, orderId, proid) {
    console.log("the status here", status)
    console.log(status, "1", orderId, "2", proid, "3")
    if (status == "placed") {
      $('#modaldiscription').text('Are you Sure Update Status to Shipped')
    } else if (status == 'shipped') {
      $('#modaldiscription').text('Are you Sure Update Status to out for delivery')
    } else if (status == "out for delivery") {
      $('#modaldiscription').text('Are you Sure Update Status to delivered')
    }
    $('#statusmodal').modal('show')

    // Adding .trim() to orderId to remove leading and trailing whitespace
    $('#shipbutton').click(() => {
      console.log("ivade eethi")
      fetch("/admin/orderstatus", {
