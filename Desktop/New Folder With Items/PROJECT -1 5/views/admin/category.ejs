  <style>
    /* Table styles */
    table {
        width: 100%;
        border-collapse: collapse;
        border: 1px solid white; /* Set table border color to white */
    }

    th, td {
        padding: 8px;
        border: 1px solid white; /* Set cell border color to white */
        text-align: left;
        color: white; /* Set text color to white */
    }

    th {
        background-color: #000000;
    }

    /* Button styles */
    .btn-1,
    .btn-2,
    .btn-3,
    .btn-4,
    .moveaside { /* Added .moveaside to target Add Category button */
        padding: 6px 10px;
        margin: 4px;
        cursor: pointer;
        border: none;
        border-radius: 4px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
        color: white; /* Set button text color to white */
    }

    .btn-1 {
        background-color: #007bff; /* Blue for Edit button */
    }

    .btn-2 {
        background-color: #dc3545; /* Red for Delete button */
    }

    .btn-3 {
        background-color: #28a745; /* Green for List button */
    }

    .btn-4 {
        background-color: #dc3545; /* Red for Unlist button */
    }

    .moveaside {
        background-color: #28a745; /* Green for Add Category button */
        float: left; /* Move the button to the left */
        margin-right: 20px; /* Add margin to create space between the button and the heading */
    }

    h2 {
        text-align: right; /* Align the heading to the right */
    }
</style>




